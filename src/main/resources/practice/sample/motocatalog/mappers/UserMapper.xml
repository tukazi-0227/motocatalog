<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="practice.sample.motocatalog.mappers.UserMapper">
  <select id="selectByUsername" resultType="practice.sample.motocatalog.beans.User">
    select 
        username
        , password
    from
        m_user
    where
        username = #{ username }
  </select>
</mapper>