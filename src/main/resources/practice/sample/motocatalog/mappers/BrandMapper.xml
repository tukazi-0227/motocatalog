<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="practice.sample.motocatalog.mappers.BrandMapper">
  <select id="selectAll" resultType="practice.sample.motocatalog.beans.Brand">
    select brand_id,
           brand_name
    from
        m_brand
    order by
        brand_id;
  </select>
</mapper>